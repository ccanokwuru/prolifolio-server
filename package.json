{
  "name": "prolifolio-server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build": "npx prisma migrate deploy && npm run build:ts",
    "build:ts": "tsc && cp -r src/utils/email dist/utils",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccanokwuru/prolifolio-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ccanokwuru/prolifolio-server/issues"
  },
  "homepage": "https://github.com/ccanokwuru/prolifolio-server#readme",
  "dependencies": {
    "@fastify/auth": "^3.0.2",
    "@fastify/autoload": "^5.0.0",
    "@fastify/bearer-auth": "^8.0.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/jwt": "^6.3.1",
    "@fastify/sensible": "^4.1.0",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@fastify/websocket": "^7.1.1",
    "@prisma/client": "^4.2.1",
    "@sinclair/typebox": "^0.24.46",
    "fast-jwt": "^1.7.1",
    "fastify": "^4.0.0",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cli": "^4.3.0",
    "fastify-formidable": "^3.0.2",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^3.0.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.8.0",
    "nunjucks": "^3.2.3",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/nunjucks": "^3.2.1",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "^4.2.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}